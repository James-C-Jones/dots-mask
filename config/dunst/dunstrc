
[global]
	geometry = "300x60-25+49"
	indicate_hidden = yes
	transparency = 10
	shrink = no
	separator_height = 0
	padding = 32
	horizontal_padding = 32
	corner_radius = 6
	#enables the frame
	frame_width = 0
	sort = no
	idle_threshold = 120
	font = rissole 8
	line_height = 4
	markup = full
	format = %s\n%b
	alignment = left
	show_age_threshold = 60
	word_wrap = yes
	ignore_newline = no
	stack_duplicates = false
	hide_duplicate_count = yes
	show_indicators = no
	icon_position = off
	sticky_history = yes
	history_length = 20
	browser = /usr/bin/qutebrowser -new-tab
	always_run_script = true
	transparency = yes
	title = Dunst 
	class = Dunst

[stack_all]
    set_stack_tag = "tag"

[shortcuts]
    mouse_left_click = do_action & close_current
    mouse_middle_click = close_all

frame_color = "#283941"
separator_color = "#a4b9af"

[low]
    msg_urgency = low
    background = "#283941"
    foreground = "#a4b9af"

[normal]
    msg_urgency = normal
    background = "#283941"
    foreground = "#f3f7f5"

[critical]
    msg_urgency = critical
    background = "#283941"
    foreground = "#da5f48"






